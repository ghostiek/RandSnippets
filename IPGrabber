using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Net;


namespace Tesing
{
    class Program
    {
        static void Main(string[] args)
        {
            string hostName = Dns.GetHostName();
            var myIP = Dns.GetHostEntry(hostName).AddressList[1];
            Console.WriteLine($"Host name is {hostName}");
            Console.WriteLine($"Your IP is {myIP}");
        }
    }
}
